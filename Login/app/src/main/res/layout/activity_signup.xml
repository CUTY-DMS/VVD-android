<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".signup">

    <Button
        android:id="@+id/join"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="180dp"
        android:text="가입"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/mPassword"
        android:layout_width="108dp"
        android:layout_height="50dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toTopOf="@+id/mName"
        app:layout_constraintEnd_toEndOf="@+id/mName"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/mName"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/mId"
        android:layout_width="108dp"
        android:layout_height="50dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toTopOf="@+id/mPassword"
        app:layout_constraintEnd_toEndOf="@+id/mPassword"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/mPassword"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/mName"
        android:layout_width="108dp"
        android:layout_height="50dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toTopOf="@+id/mAge"
        app:layout_constraintEnd_toEndOf="@+id/mAge"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/mAge"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/mAge"
        android:layout_width="108dp"
        android:layout_height="50dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toTopOf="@+id/join"
        app:layout_constraintEnd_toEndOf="@+id/join"
        app:layout_constraintStart_toStartOf="@+id/join"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>